{
    "WHERE": {
      "AND": [
        {
          "GT": {
            "sections_avg": 80
          }
        },
        {
          "IS": {
            "sections_instructor": "*i*"
          }
        },
        {
          "GT": {
            "sections_fail": 2
          }
        }
      ]
    },
    "OPTIONS": {
      "COLUMNS": [
        "sections_instructor",
        "sections_fail",
        "sumAvg",
        "minFail"
      ],
      "ORDER": {
        "dir": "DOWN",
        "keys": [
          "minFail",
          "sumAvg"
        ]
      }
    },
    "TRANSFORMATIONS": {
      "GROUP": [
        "sections_instructor",
        "sections_fail"
      ],
      "APPLY": [
        {
          "sumAvg": {
            "SUM": "sections_avg"
          }
        },
        {
          "minFail": {
            "MIN": "sections_fail"
          }
        }
      ]
    }
  }